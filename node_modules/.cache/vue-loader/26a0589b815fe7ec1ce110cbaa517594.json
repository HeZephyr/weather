\n          name=\"pattern\"\r\n\t\t\t\t\ttype=\"password\"\n          placeholder=\"请输入密码\"\n        />\n        <div style=\"margin: 16px\">\r\n\t\t\t\t\t<!-- 点击触发login方法，检测填写是否正确， 实现登录 -->\n          <van-button\n            @click=\"login\"\n            color=\"linear-gradient(to right, #0e5779, #023349)\"\n            round\n            block\n            type=\"info\"\n            native-type=\"submit\"\n          >\n            登录\n          </van-button>\n        </div>\n      </van-form>\n      <div style=\"margin: 16px\">\n        <van-button\n          color=\"linear-gradient(to right, #0e5779, #043a53)\"\n          round\n          block\n          type=\"info\"\n          @click=\"skip\"\n        >\n          游客登录\n        </van-button>\n      </div>\r\n\t\t\t<div style=\"margin: 16px\">\r\n\t\t\t  <van-button\r\n\t\t\t\t  color=\"linear-gradient(to right, #0e5779, #043a53)\"\r\n\t\t\t    round\r\n\t\t\t    block\r\n\t\t\t    type=\"info\"\r\n\t\t\t    @click=\"toRegister\"\r\n\t\t\t  >\r\n\t\t\t    去注册\r\n\t\t\t  </van-button>\r\n\t\t\t</div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\r\n\t// data函数，返回一个对象，包括正则表达式\n  data() {\n    return {\n      phone: '',\n      password: '',\r\n\t\t\t// 正则表达式，对通用手机号格式进行匹配。\n      pattern: /^(0|86|17951)?(13[0-9]|15[012356789]|166|17[3678]|18[0-9]|14[57])[0-9]{8}$/\n    };\n  },\n  methods: {\n    login() {\r\n\t\t\t// 获取当前对象复制给that。判断是否符合条件\n      const that = this;\n      if (that.phone === '' || that.password === '') {\n        that.$toast.fail('用户名或密码不能为空');\n      } else {\r\n\t\t\t\t// 通过访问应用缓存来判断是否存在这个人，通过json.parse将字符串转化为对象数组\n        const arr = JSON.parse(localStorage.getItem('users'));\n        for (let e = 0, len = arr.length; e < len; e++) {\n          if (\n            arr[e].loginName === that.phone &&\n            arr[e].loginPsd === that.password\n          ) {\n            that.$toast.success('登录成功');\n            that.$router.push('/home');\r\n\t\t\t\t\t\treturn;\n          } \n        }\r\n\t\t\t\tthat.$toast.fail('账号或密码错误');\r\n\t\t\t\tthat.phone = '';\r\n\t\t\t\tthat.password = '';\n      }\n    },\r\n\t\t// 跳转到主页面\n    skip() {\n      this.$router.push('/home');\n    },\r\n\t\t// 跳转到注册界面\n    toRegister() {\n      this.$router.push('/register');\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.box {\n  height: 100%;\n  background: radial-gradient(\n    1000% 100% at bottom center,\n    #011014,\n    #a79d8a,\n    #115b7e,\n    #06435f,\n    #032d42,\n    #01171d\n  );\n  background-attachment: fixed;\n  overflow: hidden;\n  perspective: 340px;\n\n  .stars {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    width: 2px;\n    height: 2px;\n    box-shadow: -447px 387px #c4c4c4, -401px 118px #fafafa, -109px 217px #d9d9d9,\n      -680px -436px #e3e3e3, 514px 360px #cccccc, -708px 298px #e8e8e8,\n      -696px -270px #ededed, 116px -128px #f7f7f7, 179px 35px white,\n      -404px -90px whitesmoke, -331px -309px #c4c4c4, -363px -24px #d1d1d1,\n      277px 416px #fafafa, -145px -244px whitesmoke, 123px 62px #d4d4d4,\n      -407px 418px #d9d9d9, 535px 237px #d9d9d9, -466px -78px #f7f7f7,\n      257px 287px #dedede, 327px -398px #e0e0e0, -602px -38px #c2c2c2,\n      128px 398px #e6e6e6, 274px -446px #d1d1d1, -602px -298px #c7c7c7,\n      526px -5px #c4c4c4, -90px -158px #fcfcfc, 5px 294px whitesmoke,\n      -633px 229px #c4c4c4, -475px 427px #dedede, 586px -453px #f2f2f2,\n      180px -432px #c7c7c7, -637px -88px #cfcfcf, -453px 308px #d6d6d6,\n      -111px 1px #d9d9d9, 573px -450px #ededed, 198px 300px #d6d6d6,\n      -355px 166px #dedede, -715px 13px #e3e3e3, 262px -104px #d1d1d1,\n      147px 325px #dbdbdb, 1px 399px #dbdbdb, 286px -100px white,\n      43px -329px #e8e8e8, 617px 55px #d9d9d9, -168px -392px #cccccc,\n      84px 219px #c9c9c9, 507px -226px #d9d9d9, -327px -70px #e6e6e6,\n      386px -212px #c4c4c4, -717px 4px #cfcfcf, 502px -231px #e3e3e3,\n      302px 56px #ededed, 649px 341px #c7c7c7, 569px 350px #c9c9c9